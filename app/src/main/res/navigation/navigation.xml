<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation.xml"
    app:startDestination="@id/splashScreen">

    <fragment
        android:id="@+id/home2"
        android:name="com.myloan.planner2024.Fragments.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home2_to_EMI"
            app:destination="@id/EMI" />
        <action
            android:id="@+id/action_home2_to_CompareEmi"
            app:destination="@id/CompareEmi" />
        <action
            android:id="@+id/action_home2_to_LoanAmount"
            app:destination="@id/LoanAmount" />
        <action
            android:id="@+id/action_home2_to_tenure"
            app:destination="@id/tenure" />
        <action
            android:id="@+id/action_home2_to_rateofinterest"
            app:destination="@id/rateofinterest" />
        <action
            android:id="@+id/action_home2_to_moratorium"
            app:destination="@id/moratorium" />
        <action
            android:id="@+id/action_home2_to_rdcalculation"
            app:destination="@id/rdcalculation" />
        <action
            android:id="@+id/action_home2_to_fdcalculation"
            app:destination="@id/fdcalculation" />
        <action
            android:id="@+id/action_home2_to_SICICalculate"
            app:destination="@id/SICICalculate" />
        <action
            android:id="@+id/action_home2_to_PPFCalc"
            app:destination="@id/PPFCalc" />
        <action
            android:id="@+id/action_home2_to_GSTCalc"
            app:destination="@id/GSTCalc" />
        <action
            android:id="@+id/action_home2_to_vatCal"
            app:destination="@id/vatCal" />
        <action
            android:id="@+id/action_home2_to_privacy_policy"
            app:destination="@id/privacy_policy" />
    </fragment>
    <fragment
        android:id="@+id/EMI"
        android:name="com.myloan.planner2024.Fragments.EMI"
        android:label="fragment_e_m_i"
        tools:layout="@layout/fragment_e_m_i" />
    <fragment
        android:id="@+id/CompareEmi"
        android:name="com.myloan.planner2024.Fragments.CompareEmi"
        android:label="fragmentcompareemi"
        tools:layout="@layout/fragment_compare_emi" />
    <fragment
        android:id="@+id/LoanAmount"
        android:name="com.myloan.planner2024.Fragments.ComputeLoanAmount"
        android:label="fragmentloanamount"
        tools:layout="@layout/fragment_compute_loan_amount" />
    <fragment
        android:id="@+id/tenure"
        android:name="com.myloan.planner2024.Fragments.LoanTenure"
        android:label="fragmenttenure"
        tools:layout="@layout/fragment_loan_tenure" />
    <fragment
        android:id="@+id/rateofinterest"
        android:name="com.myloan.planner2024.Fragments.CalcInterest"
        android:label="fragmentrateofintrest"
        tools:layout="@layout/fragment_calc_interest" />
    <fragment
        android:id="@+id/moratorium"
        android:name="com.myloan.planner2024.Fragments.Moratorium"
        android:label="fragmentmoratorium"
        tools:layout="@layout/fragment_moratorium" />
    <fragment
        android:id="@+id/rdcalculation"
        android:name="com.myloan.planner2024.Fragments.RDCalculation"
        android:label="fragmentrdcalc"
        tools:layout="@layout/fragment_r_d_calculation" />
    <fragment
        android:id="@+id/fdcalculation"
        android:name="com.myloan.planner2024.Fragments.FDcalculation"
        android:label="fragmentfdcalc"
        tools:layout="@layout/fragment_f_dcalculation" />
    <fragment
        android:id="@+id/SICICalculate"
        android:name="com.myloan.planner2024.Fragments.SICICalculate"
        android:label="SICICalculate" />
    <fragment
        android:id="@+id/PPFCalc"
        android:name="com.myloan.planner2024.Fragments.PPFCalc"
