        tools:layout="@layout/fragment_p_p_f_calc" />
    <fragment
        android:id="@+id/GSTCalc"
        android:name="com.myloan.planner2024.Fragments.GSTCalc"
        android:label="fragment_g_s_t_calc"
        tools:layout="@layout/fragment_g_s_t_calc" />
    <fragment
        android:id="@+id/vatCal"
        android:name="com.myloan.planner2024.Fragments.VatCal"
        android:label="fragment_vat_cal"
        tools:layout="@layout/fragment_vat_cal" />
    <fragment
        android:id="@+id/splashScreen"
        android:name="com.myloan.planner2024.Fragments.SplashScreen"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreen_to_secure"
            app:destination="@id/secure" />
    </fragment>
    <fragment
        android:id="@+id/mainPage"
        android:name="com.myloan.planner2024.Fragments.MainPage"
        android:label="fragment_main_page"
        tools:layout="@layout/fragment_main_page" >
        <action
            android:id="@+id/action_mainPage_to_home2"
            app:destination="@id/home2" />
        <action
            android:id="@+id/action_mainPage_to_privacy_policy"
            app:destination="@id/privacy_policy" />
    </fragment>
    <fragment
        android:id="@+id/privacy_policy"
        android:name="com.myloan.planner2024.Fragments.Privacy_policy"
        android:label="fragment_privacy_policy"
        tools:layout="@layout/fragment_privacy_policy" />
    <fragment
        android:id="@+id/secure"
        android:name="com.myloan.planner2024.Fragments.Secure"
        android:label="fragment_secure"
        tools:layout="@layout/fragment_secure" >
        <action
            android:id="@+id/action_secure_to_easytoUse"
            app:destination="@id/easytoUse" />
    </fragment>
    <fragment
        android:id="@+id/easytoUse"
        android:name="com.myloan.planner2024.Fragments.EasytoUse"
        android:label="fragment_easyto_use"
        tools:layout="@layout/fragment_easyto_use" >
        <action
            android:id="@+id/action_easytoUse_to_mainPage"
            app:destination="@id/mainPage" />
    </fragment>
</navigation>